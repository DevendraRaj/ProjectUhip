create table user_account (
       user_acc_id  number(10,0) not null,
        active_sw   varchar2(255 char),
        created_by  varchar2(255 char),
        create_dt   timestamp,
        user_dob    varchar2(255 char),
        user_email  varchar2(255 char),
        first_name  varchar2(255 char),
        user_gender varchar2(255 char),
        last_name   varchar2(255 char),
        user_pwd    varchar2(255 char),
        user_role   varchar2(255 char),
        ssn         number(19,0),
        updated_by  varchar2(255 char),
        update_dt   timestamp,
        primary key (user_acc_id)
    )
Hibernate: create sequence user_acc_seq start with 1 increment by  1